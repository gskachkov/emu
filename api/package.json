{
  "name": "emu-api",
  "version": "1.0.0",
  "description": "Emu RESTful API",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "start": "nodemon build/server.js",
    "start-watch": "nodemon",
    "prod": "npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "@juaning",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "debug": "^3.2.6",
    "dotenv": "^5.0.1",
    "exceljs": "^1.11.0",
    "express": "^4.17.0",
    "mongoose": "^5.5.11",
    "mongoose-validator": "^2.1.0",
    "morgan": "^1.9.1",
    "parse-error": "^0.2.0",
    "tea-school": "^1.5.0",
    "tempy": "^0.3.0",
    "written-number": "^0.9.1"
  },
  "devDependencies": {
    "@types/node": "^10.14.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.17.2",
    "nodemon": "^1.19.0"
  },
  "nodemonConfig": {
    "ignore": [
      "__tests__/*",
      "tests/*",
      "node_modules/*",
      "src/**/*.spec.ts"
    ],
    "watch": [
      "package.json",
      "tsconfig.json",
      "src"
    ],
    "ext": "ts,yaml,js,json",
    "exec": "npm run dev"
  }
}
